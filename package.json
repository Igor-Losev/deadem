{
  "name": "deadem",
  "version": "0.0.1",
  "description": "Node.js parser for Deadlock (Valve Source 2 Engine) demo/replay files",
  "author": "Igor Losev",
  "license": "MIT",
  "bugs": "https://github.com/Igor-Losev/deadem/issues",
  "homepage": "https://github.com/Igor-Losev/deadem",
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Igor-Losev/deadem"
  },
  "keywords": [
    "deadem",
    "deadlock",
    "demo",
    "demoparser",
    "parser",
    "replay"
  ],
  "main": "index.js",
  "scripts": {
    "lint": "eslint ./",
    "proto:js": "pbjs --path ./proto/source -t static-module -w commonjs --no-create --no-verify --no-delimited --no-convert -o ./proto/compiled/proto.js ./proto/source/**/*.proto",
    "proto:json": "pbjs --path ./proto/source -t json -o ./proto/compiled/proto.json ./proto/source/**/*.proto",
    "test": "jest --verbose"
  },
  "dependencies": {
    "protobufjs": "^7.4.0",
    "snappy": "^7.2.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@stylistic/eslint-plugin": "^4.2.0",
    "eslint": "^9.27.0",
    "globals": "^16.1.0",
    "jest": "^29.7.0",
    "protobufjs-cli": "^1.1.3"
  },
  "files": [
    "index.js",
    "proto/compiled/**",
    "src/**"
  ],
  "private": "false",
  "publishConfig": {
    "access": "restricted"
  }
}
